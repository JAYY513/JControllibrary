<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    >
    <SolidColorBrush x:Key="LightBrush" Color="#FFFFc32E" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="LightForegroundBrush" Color="White" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="MidBrush" Color="#FFFFB32E" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="MidForegroundBrush" Color="White" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkBrush" Color="#FFFFA02E" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkForegroundBrush" Color="White" po:Freeze="True"></SolidColorBrush>
    <SolidColorBrush x:Key="PanelBrush" Color="White" po:Freeze="True"></SolidColorBrush>
    
</ResourceDictionary>